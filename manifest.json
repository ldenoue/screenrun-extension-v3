{
    "name": "ScreenRun",
    "author": "ScreenRun LLC",
    "manifest_version": 3,
    "version": "1.1.1",
    "description": "Quickly record a video of your tab with your voice and clicks and import into ScreenRun.app",
    "options_page": "welcome.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs"],
    "content_scripts": [
        {
          "matches": ["http://localhost:8080/studio", "https://screenrun.app/studio"],
          "js": ["screenrun.js"],
          "all_frames": false,
          "run_at": "document_end"
        },
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "all_frames": false,
            "run_at": "document_end"
          }
    ],
    "action": {},
    "icons": {
        "16": "icons/off.png",
        "128": "icons/off.png",
        "48": "icons/off.png"
      }
}